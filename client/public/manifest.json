  "related_applications": [
    {
      "platform": "play",
      "url": "https://play.google.com/store/apps/details?id=com.glutenfreerecipes.app",
      "id": "com.glutenfreerecipes.app"
    },
    {
      "platform": "itunes",
      "url": "https://apps.apple.com/us/app/gluten-free-recipes/id1234567890"
    }
  ],
  "prefer_related_applications": false,
  "protocol_handlers": [
    {
      "protocol": "web+glutenfree",
      "url": "/recipe?id=%s"
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "image",
          "accept": ["image/png", "image/jpeg", "image/jpg", "image/webp"]
        }
      ]
    }
  },
  "serviceworker": {
    "src": "/sw.js",
    "scope": "/",
    "update_via_cache": "none"
  },
  "permissions": [
    "notifications",
    "geolocation",
    "camera",
    "microphone"
  ],
  "features": {
    "offline": true,
    "push_notifications": true,
    "background_sync": true,
    "payment": true,
    "credentials": true,
    "share": true
  },
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": ["navigate-existing", "auto"]
  },
  "display_override": [
    "window-controls-overlay",
    "standalone",
    "minimal-ui",
    "browser"
  ],
  "file_handlers": [
    {
      "action": "/import",
      "accept": {
        "application/pdf": [".pdf"],
        "text/plain": [".txt"],
        "application/json": [".json"]
      }
    }
  ],
  "widgets": [
    {
      "name": "Recipe of the Day",
      "description": "Daily gluten-free recipe recommendation",
      "tag": "recipe_daily",
      "ms_ac_template": "widgets/recipe-daily.json"
    },
    {
      "name": "Shopping List",
      "description": "Your gluten-free shopping list",
      "tag": "shopping_list",
      "ms_ac_template": "widgets/shopping-list.json"
    }
  ],
  "handle_links": "preferred",
  "scope_extensions": [
    {
      "origin": "*.glutenfreerecipes.com"
    }
  ]
}
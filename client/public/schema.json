    {
      "@type": "FAQPage",
      "@id": "https://glutenfreerecipes.com/faq",
      "mainEntity": [
        {
          "@type": "Question",
          "name": "What is celiac disease?",
          "acceptedAnswer": {
            "@type": "Answer",
            "text": "Celiac disease is an autoimmune disorder where gluten consumption damages the small intestine lining. It affects about 1% of the population and requires a strict gluten-free diet for life."
          }
        },
        {
          "@type": "Question",
          "name": "How do I prevent cross-contamination?",
          "acceptedAnswer": {
            "@type": "Answer",
            "text": "Use separate cutting boards, toasters, and utensils. Clean surfaces thoroughly. Store gluten-free items above gluten-containing foods. Use dedicated gluten-free cooking equipment when possible."
          }
        },
        {
          "@type": "Question",
          "name": "What flour can replace all-purpose flour?",
          "acceptedAnswer": {
            "@type": "Answer",
            "text": "No single flour perfectly replaces all-purpose flour. Use blends combining rice flour, tapioca starch, and xanthan gum. Commercial 1:1 baking blends work well for most recipes."
          }
        }
      ]
